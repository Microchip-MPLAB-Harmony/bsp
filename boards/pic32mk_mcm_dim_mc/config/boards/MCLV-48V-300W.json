{
    "MCLV-48V-300W": {
      "Buttons": {
        "BUTTON_01": {
          "debouncing": {
            "enabled": true,
            "delay_ms": 50
          },
          "gpio_config": {
            "pin_number": "RD15",
            "pull_up": true,
            "active_low": false
          }
        },
        "BUTTON_02": {
          "debouncing": {
            "enabled": true,
            "delay_ms": 50
          },
          "gpio_config": {
            "pin_number": "RD2",
            "pull_up": true,
            "active_low": false
          }
        }
      },
      "Leds": {
        "LED_01": {
          "gpio_config": {
            "pin_number": "RG0",
            "active_low": false
          }
        },
        "LED_02": {
          "gpio_config": {
            "pin_number": "RG1",
            "active_low": false
          }
        }
      },
      "Analog_Circuits": {
        "IA": {
          "type": "shunt_with_opamp",
          "circuit_param": {
            "shunt_resistor": 0.003,
            "opamp_gain": 25.0,
            "opamp_bias": 1.65
          },
          "peripheral": {
            "instance": "ADC0",
            "channel": 0
          },
          "gpio_config": {
            "pin_number": "RA0"
          }
        },
        "IB": {
          "type": "shunt_with_opamp",
          "circuit_param": {
            "shunt_resistor": 0.003,
            "opamp_gain": 25.0,
            "opamp_bias": 1.65
          },
          "peripheral": {
            "instance": "ADC3",
            "channel": 3
          },
          "gpio_config": {
            "pin_number": "RB1"
          }
        },
        "VPOT": {
          "type": "buffer",
          "circuit_param": {},
          "peripheral": {
            "instance": "ADC7",
            "channel": 16
          },
          "gpio_config": {
            "pin_number": "RG9"
          }
        },
        "VDC": {
          "type": "voltage_divider",
          "circuit_param": {
            "top_resistor": 66.0,
            "bottom_resistor": 3.0
          },
          "peripheral": {
            "instance": "ADC7",
            "channel": 13
          },
          "gpio_config": {
            "pin_number": "RE13"
          }
        }
      },
      "Position_Interface": {
        "QDEC": {
            "type": "optical_encoder",
            "peripheral": {
                "instance": "QEI1",
                "channel": "1"
            },
            "gpio_pins": {
                "qea": {
                    "gpio_config": {
                        "pin_number": "RC7"
                    }
                },
                "qeb": {
                    "gpio_config": {
                        "pin_number": "RA8"
                    }
                }
            }
        }
      },
      "Inverter": {
        "INVERTER": {
          "peripheral": {
            "instance": "MCPWM",
            "channels": {
              "phase_a": 1,
              "phase_b": 2,
              "phase_c": 3
            }
          },
          "gpio_pins": {
            "phase_a": {
              "low_side": {
                "gpio_config": {
                  "pin_number": "RB15"
                }
              },
              "high_side": {
                "gpio_config": {
                  "pin_number": "RB14"
                }
              }
            },
            "phase_b": {
              "low_side": {
                "gpio_config": {
                  "pin_number": "RB13"
                }
              },
              "high_side": {
                "gpio_config": {
                  "pin_number": "RB12"
                }
              }
            },
            "phase_c": {
              "low_side": {
                "gpio_config": {
                  "pin_number": "RB11"
                }
              },
              "high_side": {
                "gpio_config": {
                  "pin_number": "RB10"
                }
              }
            },
            "fault": {
              "gpio_config": {
                "pin_number": "RC8"
              }
            }
          },
          "voltage_rating": 24,
          "current_rating": 5,
          "control_mode": "PWM",
          "max_pwm_frequency": 20000
        }
      },
      "Communication": {
        "X2C_PORT": {
          "type": "two_wired_uart",
          "peripheral": {
            "instance": "UART1",
            "transmit_channel":"1",
            "receive_channel": "1"
          },
          "gpio_pins": {
            "transmit": {
              "gpio_config": {
                "pin_number": "RB4"
              }
            },
            "receive": {
              "gpio_config": {
                "pin_number": "RC13"
              }
            }
          }
        }
      }
    }
}
